#вариант1
items = [1, 2, 3, 4, 5]
new_items = []
for i in range(1, len(items)):
    a, b = items[i - 1], items[i]
    if b > a:
             new_items.append(b) 
print(new_items) 

#вариант2

my_list = [300, 2, 12, 44, 1, 1, 4, 10, 7, 1, 78, 123, 55]
new_list = [number for i, number in enumerate(my_list) if i > 0 and my_list[i] > my_list[i - 1]]
print(new_list) 
